        },
        "additionalProperties": false
      },
      "ChartType": {
        "enum": [
          0,
          1,
          2,
          3,
          4
        ],
        "type": "integer",
        "format": "int32"
      },
      "CreatePlanetRequest": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "description": {
            "type": "string",
            "nullable": true
          },
          "location": {
            "type": "string",
            "nullable": true
          },
          "discoveredDate": {
            "type": "string",
            "format": "date-time"
          }
        },
        "additionalProperties": false
      },
      "Evaluation": {
        "required": [
          "planetIdsJson",
          "weightsJson"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "planetIdsJson": {
            "minLength": 1,
            "type": "string"
          },
          "algorithm": {
            "$ref": "#/components/schemas/EvaluationAlgorithm"
          },
          "weightsJson": {
            "minLength": 1,
            "type": "string"
          },
          "createdAt": {
            "type": "string",
            "format": "date-time"
          },
          "createdBy": {
            "maxLength": 100,
            "type": "string",
            "nullable": true
          },
          "createdByUserId": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "status": {
            "$ref": "#/components/schemas/EvaluationStatus"
          },
          "results": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/EvaluationResult"
            },
            "nullable": true
          },
          "report": {
            "$ref": "#/components/schemas/EvaluationReport"
          },
          "createdByUser": {
            "$ref": "#/components/schemas/User"
          },
          "planetIds": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            },
            "nullable": true
          },
          "weights": {
            "type": "object",
            "additionalProperties": {
              "type": "number",
              "format": "double"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EvaluationAlgorithm": {
        "enum": [
          0,
          1,
          2,
          3
        ],
        "type": "integer",
        "format": "int32"
      },
      "EvaluationReport": {
        "required": [
          "title"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "evaluationId": {
            "type": "integer",
            "format": "int32"
          },
          "title": {
            "maxLength": 500,
            "minLength": 1,
            "type": "string"
          },
          "summary": {
            "type": "string",
            "nullable": true
          },
          "detailedAnalysis": {
            "type": "string",
            "nullable": true
          },
          "generatedAt": {
            "type": "string",
            "format": "date-time"
          },
          "evaluation": {
            "$ref": "#/components/schemas/Evaluation"
          },
          "charts": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/Chart"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EvaluationRequest": {
        "type": "object",
        "properties": {
          "planetIds": {
            "type": "array",
            "items": {
              "type": "integer",
              "format": "int32"
            },
            "nullable": true
          },
          "algorithm": {
            "$ref": "#/components/schemas/EvaluationAlgorithm"
          },
          "weights": {
            "type": "object",
            "additionalProperties": {
              "type": "number",
              "format": "double"
            },
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "EvaluationResult": {
        "required": [
          "categoryScoresJson",
          "risksJson",
          "strengthsJson",
          "weaknessesJson"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "evaluationId": {
            "type": "integer",
            "format": "int32"
          },
          "planetId": {
            "type": "integer",
            "format": "int32"
          },
          "totalScore": {
            "type": "number",
            "format": "double"
          },
          "categoryScoresJson": {
            "minLength": 1,
            "type": "string"
          },
          "rank": {
            "type": "integer",
            "format": "int32"
          },
          "recommendation": {
            "type": "string",
            "nullable": true
          },
          "strengthsJson": {
            "minLength": 1,
            "type": "string"
          },
          "weaknessesJson": {
            "minLength": 1,
